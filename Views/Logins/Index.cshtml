@model IEnumerable<MVCTilausDB.Models.Logins>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PassWord)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LoginErrorMessage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegisterationDate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PassWord)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoginErrorMessage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegisterationDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.LoginId }) |
                @Html.ActionLink("Details", "Details", new { id = item.LoginId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.LoginId })
            </td>
        </tr>
    }

</table>
